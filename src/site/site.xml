<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
         name="Liferay Spring Extensions">

    <body>
        <breadcrumbs>
            <item href="http://www.permeance.com.au" name="Permeance Technologies"/>
            <item href="index.html" name="Liferay Spring Extensions"/>
        </breadcrumbs>
        <menu ref="reports"/>
    </body>

    <custom>
        <fluidoSkin>
            <bodyColumnClass>span10</bodyColumnClass>
            <facebookLike/>
            <googlePlusOne/>
            <gitHub>
                <projectId>permeance/liferay-spring-extensions</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
            <leftColumnClass>span2</leftColumnClass>
            <ohloh>
                <projectId>liferay-spring-extensions</projectId>
                <widget>stats</widget>
            </ohloh>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <twitter>
                <showFollowers>true</showFollowers>
                <showUser>true</showUser>
                <user>permtech</user>
            </twitter>
        </fluidoSkin>
    </custom>

    <googleAnalyticsAccountId>UA-6598203-1</googleAnalyticsAccountId>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>

</project>
